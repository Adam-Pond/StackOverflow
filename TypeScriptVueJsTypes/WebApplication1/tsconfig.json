{
  "compilerOptions": {
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "sourceMap": true,
    "typeRoots": [
      "./typings/"
    ],
    "allowSyntheticDefaultImports": true,
    "target": "es5",
    "lib": [ "es5", "dom", "es2015.promise" ]
  },
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
